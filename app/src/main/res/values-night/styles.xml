<resources>

    <style name="ButtonRoundedRect" parent="Widget.Material3.Button.ElevatedButton">
        <item name="backgroundTint">?attr/colorPrimary</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.SmallComponent.12</item>
        <item name="minHeight">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="rippleColor">@color/material_dynamic_primary40</item>
    </style>

    <style name="ButtonIcon" parent="Widget.Material3.Button.TextButton">
        <item name="iconTint">@null</item>
        <item name="iconPadding">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="cornerRadius">0dp</item>
        <item name="iconSize">24dp</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="android:backgroundTint">@android:color/transparent</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:minWidth">56dp</item>
        <item name="android:minHeight">56dp</item>
    </style>

    <style name="ShapeAppearance.App.SmallComponent.12" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerSize">12dp</item>
    </style>

    <style name="Widget.Theme.VehicleComplaintReportMiniTest.MyView" parent="">
        <item name="android:background">@color/gray_600</item>
        <item name="exampleColor">@color/light_blue_600</item>
    </style>

    <style name="DialogOverlayTheme" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:backgroundTint">@color/gray_600</item>
        <item name="android:windowBackground">@drawable/background_rounded_10_radius</item>
        <item name="android:windowMinWidthMinor">0%</item>
    </style>
</resources>